{
  "version": "0.0.6",
  "tasks": {
    "dev": "deno run --allow-read --allow-write --allow-net --allow-env --allow-run src/tui.ts",
    "run": "deno run --allow-read --allow-write --allow-net --allow-env --allow-run src/run.ts",
    "curl2http": "deno run --allow-read --allow-write --allow-env scripts/curl2http.ts",
    "init": "deno run --allow-read --allow-write --allow-env src/init.ts",
    "build": "deno compile --allow-read --allow-write --allow-net --allow-env --allow-run --output bin/restcli src/tui.ts",
    "build:curl2http": "deno compile --allow-read --allow-write --allow-net --allow-env --allow-run --output bin/restcli-curl2http scripts/curl2http.ts",
    "build:openapi2http": "deno compile --allow-read --allow-write --allow-net --allow-env --allow-run --output bin/restcli-openapi2http scripts/openapi2http.ts",
    "build:all": "deno task build && deno task build:curl2http && deno task build:openapi2http"
  },
  "imports": {
    "@std/fmt": "jsr:@std/fmt@1",
    "@std/fs": "jsr:@std/fs@1",
    "@std/path": "jsr:@std/path@1",
    "@std/yaml": "jsr:@std/yaml@1"
  }
}
